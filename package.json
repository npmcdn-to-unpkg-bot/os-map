{
  "name": "tstibbs_os-map",
  "version": "0.0.1",
  "scripts": {
    "test": "mocha",
    "test_coverage_baseline": "node ./node_modules/istanbul/lib/cli.js --hook-run-in-context cover ./node_modules/mocha/bin/_mocha node_modules/istanbul-baseline/cli.js ./js/* --report lcovonly -- -R spec && node node_modules/lcov-zeroer/cli.js ./coverage/lcov.info ./coverage/lcov.info.baseline",
    "test_coverage": "node ./node_modules/istanbul/lib/cli.js --hook-run-in-context cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "merge_test_coverage": "node_modules/.bin/lcov-result-merger 'coverage/lcov.info*' 'coverage/lcov.merged'",
    "coveralls": "cat ./coverage/lcov.merged | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "tstibbs",
  "license": "Apache-2.0",
  "homepage": "https://github.com/tstibbs/os-map",
  "dependencies": {
    "istanbul": "^0.4.3",
    "coveralls": "^2.11.9",
    "lcov-result-merger": "^1.2.0",
    "requirejs": "^2.2.0",
    "mocha": "^2.4.5",
    "should": "^8.3.1",
    "istanbul-baseline": "tstibbs/node-istanbul-baseline",
    "lcov-zeroer": "tstibbs/node-lcov-zeroer",
    "proj4": "^2.3.14"
  }
}
